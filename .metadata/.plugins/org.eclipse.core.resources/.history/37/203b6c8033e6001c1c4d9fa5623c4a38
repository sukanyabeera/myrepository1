package com.Exceptions;

public class HandleNumberFormateException {

	public static void main(String[] args) {
		String price="200";
		int discount=10;
		
		try {
			int a=Integer.parseInt(price);
			System.out.println("discount is "+a/discount);
		}
		catch(Exception e) {
			System.out.println(e);
		}
		
		
	}
}
