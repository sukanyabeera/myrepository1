package com.automobile.twowheeler;

import com.automobile.Vehicle;

public class Hero extends Vehicle {

	public String getModelName() {
		System.out.println(" Hero model name is ");
		return "Passion pro";
	}

	public String getRegistrationNumber() {
		System.out.println(" Registration number for Hero is ");
		return "TS45283";
	}
	public String getownerName() {
		
		System.out.println(getSpeed());
		radio();
		System.out.println("owner of the Hero is ");
		return "sukanya";
	}
	public int getSpeed() 
	{
		System.out.println("The current speed of the Hero is ");
		return 120;	
	}

	public void radio() {
		
		System.out.println("Hero provides facility to control the radio device ");
	}
}
