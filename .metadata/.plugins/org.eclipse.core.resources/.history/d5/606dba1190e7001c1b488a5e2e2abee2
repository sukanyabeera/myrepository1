package com.factoryMethodProgram;

import com.interfacres.OfficeFurniture;

//import com.interfacres.OnlineFurniture;

interface Furniture{
	
	void selectTheBrand();
	void selectItem();
	void numberOfItems();
	void makeAnOrder();
}
class OfficeFurniture implements Furniture{
	
	
	public static void selectTheBrand(String brand){
		
		System.out.println("selected  brand");
	}
	public void selectItem() {
		
		System.out.println("selected item");
	}
	public void numberOfItems() {
		System.out.println("thank you!");
	}
	public void makeAnOrder() {
		 System.out.println("order conformed");
		// System.out.println("we will deliver you "+number+" "+item+"s 0f "+brand+" brand");
	}
	@Override
	public void selectTheBrand() {
		// TODO Auto-generated method stub
		
	}
}

class FurnitureFactory1{
	
	Furniture getFurniture(String furniture)
	{
		if(furniture.equals("OfficeFurniture")) {
			return new OfficeFurniture();
		}
		else {
		return null;
	}
	
}

public  FactoryMethodProgram {
	OfficeFurniture office=new OfficeFurniture();
	office.selectTheBrand("ikea");
	office.selectItem("chair");
}

