		if(age<12) {
			System.out.println("You have successfully registered under  Kids Account");
			requestBook();		}
		else
			AdultUsers au=new AdultUsers();
		// System.out.println("enter the age");
		//au.age=scan.nextInt();
	//	au.registerAccount();
	}

	@Override
	public void requestBook() {
		System.out.println("The book name :"+bookType);
		
		
	}

}
