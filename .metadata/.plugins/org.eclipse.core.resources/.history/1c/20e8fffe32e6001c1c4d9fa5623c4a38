package com.Exceptions;

public class HandleNumberFormateException {

	public static void main(String[] args) {
		//String price="200";
		//int discount=10;
		
		try {
			System.out.println("discount is "+Integer.parseInt("sukanya"));
		}
		catch(Exception e) {
			System.out.println(e);
		}
		
		
	}
}
