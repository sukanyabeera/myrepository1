package com.automobile.twowheeler;

import com.automobile.Vehicle;

class VehicleFactory{
	static nameOfVehicle;
	Vehicle getVehicle(String nameOfVehicle) {
		this.nameOfVehicle=nameOfVehicle;
		if(nameOfVehicle.equals("Honda"))
		{
			return new Honda();
		}
		else if(nameOfVehicle.equals("Hero"))
		{
			return new Hero();
		}
		else 
		return null;
	}
}

public class Test {

	public static void main(String[] args) {
		VehicleFactory vf=new VehicleFactory();
		Vehicle v=vf.getVehicle("Honda");
		System.out.println(v.getModelName());
		System.out.println(v.getRegistrationNumber());
		System.out.println(v.getownerName());
		Hero hero=new Hero();
		Honda honda=new Honda();
		if(nameOfVehicle.equals("Hero")) {
			System.out.println(hero.getSpeed());	
		}
		else if(v.equals("Honda")) {
			System.out.println(honda.getSpeed());
		}
		
		
		
		
		

	}

}
