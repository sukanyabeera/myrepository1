package com.automobile.twowheeler;

import com.automobile.Vehicle;

public class Honda extends Vehicle{

	
	public String getModelName() {
		System.out.println(" Honda model name is ");
		return "Honda City";
	}

	public String getRegistrationNumber() {
		System.out.println(" Registration number for honda is ");
		return "TSHonda145";
	}

	public String getownerName() {
		System.out.println("owner of the honda is ");
		System.out.println(getSpeed());
		return "shiny";
	}
	public int getSpeed() 
	{
		System.out.println("The current speed of the Honda is ");
		return 400;
	}
	public void cdplayer()
	{
		System.out.println("Honda provides facility to control the cd player device which is available in the car. ");
	}



}
